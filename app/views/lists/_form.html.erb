<div class="center-align container">
	<%= form_for @list, :html => { :multipart => true } do |f| %>

		<div class="row">
			<div class="input-field col s4 offset-s4">
			  <%= f.label :title %>
			  <%= f.text_field :title %>
		  </div>
		</div>

		<div class="row">
			<div class="input-field col s4 offset-s4">
			  <%= f.label :description %>
			  <%= f.text_field :description %>
		  </div>
	  </div>

	  <%= f.submit %>
	<% end %>
</div>